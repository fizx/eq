<h1>Listing web_calendars</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Data</th>
  </tr>

<% @web_calendars.each do |web_calendar| %>
  <tr>
    <td><%=h web_calendar.url %></td>
    <td><%=h web_calendar.data %></td>
    <td><%= link_to 'Show', web_calendar %></td>
    <td><%= link_to 'Edit', edit_web_calendar_path(web_calendar) %></td>
    <td><%= link_to 'Destroy', web_calendar, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New web_calendar', new_web_calendar_path %>